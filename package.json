{
  "name": "scitran-organizer",
  "productName": "SciTran Organizer",
  "version": "0.0.1",
  "description": "SciTran Data Organizer",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "dev": "electron . --enable-logging",
    "clean": "rm -rf ./dist",
    "clean:osx": "rm -rf ./dist/osx",
    "clean:win": "rm -rf ./dist/win",
    "pack": "npm run clean && npm run pack:osx && npm run pack:win",
    "pack:osx": "npm run clean:osx && electron-packager . --out=dist/osx --platform=darwin --arch=x64 --version=0.37.8 --icon=assets/osx/logo.icns --ignore=dist --ignore=assets --ignore=builder.json --ignore=bower.json --ignore=README.md --ignore=.gitignore --ignore=preview.png",
    "pack:win": "npm run clean:win && electron-packager . --out=dist/win --platform=win32 --arch=ia32 --version=0.37.8 --icon=assets/win/logo.ico --ignore=dist --ignore=assets --ignore=builder.json --ignore=bower.json --ignore=README.md --ignore=.gitignore --ignore=preview.png",
    "build": "npm run build:osx && npm run build:win",
    "build:osx": "npm run pack:osx && electron-builder \"dist/osx/*-darwin-x64/*.app\" --platform=osx --out=\"dist/osx\" --config=builder.json",
    "build:win": "npm run pack:win && electron-builder \"dist/win/*-win32-ia32\" --platform=win --out=\"dist/win\" --config=builder.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scitran/organizer.git"
  },
  "keywords": [
    "SciTran",
    "Data",
    "Organizer",
    "DICOM",
    "NIfTI",
    "BIDS",
    "OpenfMRI"
  ],
  "author": "SciTran Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scitran/organizer/issues"
  },
  "homepage": "https://github.com/scitran/organizer#readme",
  "devDependencies": {
    "electron-packager": "^7.0.1",
    "electron-prebuilt": "^0.37.8"
  }
}
